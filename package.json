{
  "name": "my-vite-template",
  "version": "1.2.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "pnpm:check": "NPM_CHECK_INSTALLER=pnpm npm-check -u",
    "type:check": "vue-tsc --noEmit ",
    "lint:check": "eslint \"**/*.{vue,ts,js}\"",
    "lint:fix": "eslint \"**/*.{vue,ts,js}\" --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "serve": "vite preview",
    "pocketbase:serve": "./db/pocketbase serve",
    "pocketbase:types": "npx pocketbase-typegen --db ./db/pb_data/data.db --out ./src/pocketbase-types.ts"
  },
  "dependencies": {
    "@vueuse/core": "10.4.1",
    "mapbox-gl": "^2.15.0",
    "pocketbase": "^0.17.3",
    "vue": "3.3.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.41.0",
    "@types/geojson": "^7946.0.10",
    "@types/jest": "^29.5.4",
    "@types/mapbox-gl": "^2.7.13",
    "@types/node": "^20.5.9",
    "@vitejs/plugin-vue": "4.3.4",
    "@vitest/coverage-c8": "0.33.0",
    "@vitest/ui": "0.34.3",
    "@vue/test-utils": "2.4.1",
    "autoprefixer": "10.4.15",
    "c8": "8.0.1",
    "eslint": "8.48.0",
    "happy-dom": "10.11.2",
    "http-server": "14.1.1",
    "msw": "1.3.0",
    "npm-check": "6.0.1",
    "pocketbase-typegen": "^1.1.13",
    "postcss": "8.4.29",
    "sass": "1.66.1",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2",
    "unplugin-auto-import": "0.16.6",
    "unplugin-vue-components": "0.25.2",
    "vite": "4.4.9",
    "vite-plugin-pwa": "^0.16.4",
    "vite-plugin-windicss": "1.9.1",
    "vitest": "0.34.3",
    "vue-tsc": "1.8.8",
    "windicss": "3.5.6"
  },
  "msw": {
    "workerDirectory": "public"
  }
}