{
  "name": "forager",
  "version": "2.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "tailwind-config-viewer": "tailwind-config-viewer -o",
    "pnpm:check": "pnpm up -L -i",
    "type:check": "vue-tsc --noEmit ",
    "lint:check": "eslint \"src/*.{vue,ts,js}\"",
    "lint:fix": "eslint \"src/*.{vue,ts,js}\" --fix",
    "serve": "vite preview",
    "pocketbase:serve": "cd db && go run main.go serve",
    "pocketbase:seed":"cd db && go run main.go seed",
    "pocketbase:types": "npx pocketbase-typegen --db ./db/pb_data/data.db --out ./src/pocketbase-types.ts",
    "changeset": "npx changeset",
    "changeset:status": "npx changeset status --verbose",
    "changeset:version": "npx changeset version"
  },
  "dependencies": {
    "@vueuse/core": "10.4.1",
    "mapbox-gl": "^2.15.0",
    "pinia": "^2.1.6",
    "pocketbase": "^0.18.0",
    "tippy.js": "^6.3.7",
    "vue": "3.3.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.43.0",
    "@changesets/cli": "^2.26.2",
    "@types/geojson": "^7946.0.10",
    "@types/mapbox-gl": "^2.7.14",
    "@types/node": "^20.6.2",
    "@vitejs/plugin-vue": "4.3.4",
    "autoprefixer": "10.4.15",
    "eslint": "8.49.0",
    "pocketbase-typegen": "^1.1.13",
    "postcss": "8.4.30",
    "sass": "1.67.0",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2",
    "unplugin-auto-import": "0.16.6",
    "unplugin-vue-components": "0.25.2",
    "vite": "4.4.9",
    "vite-plugin-pwa": "^0.16.5",
    "vue-tsc": "1.8.11"
  }
}